! gnuplot
gnuplot*background: white
gnuplot*textColor: black
gnuplot*borderColor: black
gnuplot*axisColor: black
gnuplot*line1Color: red
gnuplot*line2Color: green
gnuplot*line3Color: blue
gnuplot*line4Color: magenta
gnuplot*line5Color: cyan
gnuplot*line6Color: sienna
gnuplot*line7Color: orange
gnuplot*line8Color: coral

! Emacs
Emacs.background: ivory
Emacs.foreground: MidnightBlue

Emacs.modeline*attributeForeground:     MidnightBlue
Emacs.modeline*attributeBackground:     Bisque

Emacs.text-cursor*attributeBackground:  Red3
Emacs.pointer*attributeForeground:      Red3
Emacs.pointer*attributeBackground:      Red3
Emacs*menubar*Foreground:               MidnightBlue
Emacs*menubar*Background:               Bisque1
Emacs*popup*Foreground:                 MidnightBlue
Emacs*popup*Background:                 Bisque1

Emacs.default.attributeFont:  -adobe-courier-medium-r-*-*-12-*-*-*-*-*-iso8859-*

! xpdf -----------------------------------------------------------------------

xpdf*enableFreetype: True
xpdf*antialias: True
xpdf*Background: thistle4
!xpdf*Background: light-blue
xpdf*Foreground: Black
xpdf.paperColor: White
xpdf.matteColor: #D7D7D7
!xpdf.matteColor: Tan
xpdf.fullScreenMatteColor: #D7D7D7
xpdf.initialZoom: width
xpdf*geometry: 1100x800
xpdf.enableT1lib: True
!xpdf.t1libControl: none
xpdf*urlCommand: /usr/bin/firefox %s
xpdf.continuousView: True
xpdf.toolTipEnable: true

! xdvi ------------------------------------------------------------------------
xdvi*font: -*-helvetica-medium-r-*-*-12-*-*-*-*-*-*-*
xdvi*statusline*font: -*-helvetica-medium-r-*-*-11-*-*-*-*-*-*-*
xdvi*toolbarButtonsRaised: True
xdvi*button*fillOnArm: True
xdvi*button*shadowThickness: 1
xdvi*toolBar.marginHeight: 0
xdvi*toolBarFrame.marginWidth: 0
xdvi*button*highlightThickness: 2
xdvi*buttonBorderWidth: 2
!XDvi*thorough: true
XDvi*foreground: #202020
XDvi*background: White
!xdvi*background: #D7D7D7
!XDvi*underLink: true
XDvi*pixelsPerInch: 1200
XDvi*shrinkFactor: 8
XDVI*font: ter-120b

